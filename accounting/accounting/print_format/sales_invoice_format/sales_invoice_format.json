 "html": "<div class=\"print__parent\">\n  <div class=\"border-bottom print__header\">\n  <h1 class=\"float-end display-1\">{{ _(\"Sales Invoice\") }}</h1>\n  <hr/>\n  <div class=\"float-end\"><b>{{_(\"Transaction Title:\")}} </b>{{ _(doc.title) }}</div><br/>\n   <div class=\"display-3\"><b> {{ _(\"Customer:\") }} </b> {{ doc.party }} </div><br/>\n   <div class=\"display-3\"><b> {{ _(\"Posting Date:\") }} </b> {{doc.posting_date}}</div><br/>\n  </div>\n  <hr/>\n  <table class=\"table table-bordered\">\n    <thead>\n      <tr>\n        <th>{{ _(\"#\")}}</th>\n        <th>{{ _(\"Item\") }}</th>\n        <th class=\"text-right\">{{ _(\"Qty\") }}</th>\n        <th class=\"text-right\">{{ _(\"Rate\") }}</th>\n        <th class=\"text-right\">{{ _(\"Amount\") }}</th>\n      </tr>\n    </thead>\n    <tbody>\n      {% for row in doc.items %}\n      <tr>\n        <td class=\"text-right\">{{ row.idx }}</td>\n        <td class=\"text-right\">{{ row.item }}</td>\n        <td class=\"text-right\">{{ row.quantity }}</td>\n        <td class=\"text-right\">{{ frappe.format(row.rate, 'Currency') }}</td>\n        <td class=\"text-right\">{{ frappe.format(row.total, 'Currency') }}</td>\n      </tr>\n      {% endfor %}\n    </tbody>\n  </table>\n  <div class=\"row print__rows\">\n        <div class=\"col-sm-6 print__col\">\n          <b> {{ _(\"Total Amount:\") }} </b> {{ frappe.format(doc.total,\n          'Currency')}}\n        </div>\n</div>",
